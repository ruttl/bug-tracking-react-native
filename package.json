{
  "name": "@ruttl/bug-tracking",
  "version": "1.0.34",
  "description": "React Native plugin for bug tracking via Ruttl",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "source": "src/index.js",
  "types": "lib/types/commonjs/index.d.ts",
  "react-native": "src/index.js",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "build": "npm run clean && bob build",
    "clean": "rimraf lib",
    "prepare": "npm run build"
  },
  "keywords": [
    "ruttl",
    "bug trakcing"
  ],
  "repository": "https://github.com/ruttl/bug-tracking-react-native.git",
  "bugs": "https://github.com/ruttl/bug-tracking-react-native/issues",
  "homepage": "https://ruttl.com/mobile-app-feedback/",
  "license": "ISC",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "copyFlow": false,
          "sourceMaps": false
        }
      ],
      [
        "module",
        {
          "sourceMaps": false
        }
      ]
    ]
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-native": "^0.71.7",
    "react-native-builder-bob": "^0.20.4",
    "rimraf": "^5.0.10"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.71.0",
    "react-native-btr": "^2.2.1",
    "react-native-builder-bob": "^0.20.4",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-svg": "^13.9.0",
    "react-native-view-shot": "^3.6.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "prop-types": "^15.8.1",
    "react-native-btr": "^2.2.1",
    "react-native-draggable": "^3.3.0",
    "react-native-material-ripple": "^0.9.1",
    "react-native-toast-message": "^2.1.6"
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
